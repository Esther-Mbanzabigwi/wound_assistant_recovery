{
  "expo": {
    "name": "wound_assistant_recovery",
    "slug": "mothermend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "woundassistantrecovery",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "config": {
        "googleMapsApiKey": "AIzaSyBVkev2Nxc3OfWXY0bv1J0UrhXgmZsX0iw"
